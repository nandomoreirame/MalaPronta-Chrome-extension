{
  "manifest_version": 2,

  "name": "Encontre seu hotel! by MalaPronta.com",
  "description": "Hotéis no Brasil e no Mundo pelos melhores preços MalaPronta.com",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Encontre seu hotel!"
  },
  "icons" : {
    "16" : "images/icon16.png",
    "48" : "images/icon48.png",
    "128" : "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
